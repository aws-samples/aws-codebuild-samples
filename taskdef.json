{
  "executionRoleArn": "arn:aws:iam::133150622354:role/codedeploy-for-ecs-ecs-task-execution",
  "containerDefinitions": [
    {
      "name": "generic",
      "image": "<IMAGE_NAME>",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "oeg"
}